/*文字截断  */

.ellipsis, .ellipsis-line2, .ellipsis-line3, .ellipsis-line4 {
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.ellipsis-line2 {
    -webkit-line-clamp: 2;
}

.ellipsis-line3 {
    -webkit-line-clamp: 3;
}

